es-2) == -1 && <PageNumber>...</PageNumber>}\r\n      <PageNumber\r\n        active={props.page.current==(totalPages)}\r\n        onClick={()=> props.changePage(totalPages)}\r\n      >{totalPages}</PageNumber>\r\n      {props.page.current !== totalPages && <IconButton onClick={props.clickNext}>{\"Next\"}</IconButton>}\r\n      {props.page.current !== totalPages && <IconButton onClick={()=> props.changePage(totalPages)}>{\">>\"}</IconButton>}\r\n    </PaginationWrapper>\r\n  )\r\n}\r\n\r\nexport default DefaultPagination;","C:\\Users\\danie\\Workspace\\client-arte-en-denim\\src\\components\\Pagination\\style.js",[],"C:\\Users\\danie\\Workspace\\client-arte-en-denim\\src\\components\\Pagination\\MaterialPagination.js",[],"C:\\Users\\danie\\Workspace\\client-arte-en-denim\\src\\components\\Table\\DataTableFooter.js",["508"],"import React, {useState, useEffect} from 'react';\r\nimport {MaterialPagination} from '../Pagination';\r\nimport {TableFooterWrapper} from './style';\r\n\r\nconst DataTableFooter = (props) => {\r\n  const [totalPages, setTotalPages] = useState(0);\r\n\r\n  useEffect(() => {\r\n    setTotalPages(Math.ceil(props.data.rows.length / props.page.per))\r\n  }, [props.data.rows])\r\n\r\n  return (\r\n    <TableFooterWrapper>\r\n      <div>\r\n        mostrando del\r\n        <b> {props.page.range.min+1}</b> al <b>{props.page.range.max}</b> -\r\n        de <b>{props.data.rows.length || 0}</b> resultados\r\n      </div>\r\n      <MaterialPagination\r\n        pages={totalPages}\r\n        current={props.page.current} \r\n        disabled={props.disabled}\r\n        onChange={props.onChange}/>\r\n    </TableFooterWrapper>\r\n  )\r\n}\r\n\r\nexport default DataTableFooter;","C:\\Users\\danie\\Workspace\\client-arte-en-denim\\src\\pages\\session\\login.js",["509","510","511","512"],"import React, {useState, useEffect} from 'react';\r\nimport {connect} from 'react-redux';\r\nimport {useHistory} from 'react-router-dom';\r\n\r\nimport {Page} from '../../components